{
  "name": "forager",
  "version": "0.5.0",
  "description": "",
  "type": "commonjs",
  "main": "src/index.js",
  "bin": {
    "forager": "src/cli.js"
  },
  "scripts": {
    "build": "cp package.json build/src && sed -i.bak 's/..\\/..\\/package.json/..\\/package.json/' build/src/db/sqlite.js",
    "build": "tsc",
    "build:watch": "mkdir -p build/src && cp package.json build/src && tsc --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "jest --config test/jest.config.json",
    "test:migrations": "npm run test -- test/migrations/*test.ts",
    "test:watch": "npm run test -- test/**/*test.ts --watch"
  },
  "keywords": [],
  "author": "Andrew Kaiser",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.1",
    "better-sqlite3": "^7.4.3",
    "date-fns": "^2.23.0",
    "sharp": "^0.28.3",
    "zod": "^3.8.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.4.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^15.14.2",
    "@types/sharp": "^0.28.5",
    "@types/tap": "^15.0.5",
    "@types/tape": "^4.13.2",
    "jest": "^27.4.5",
    "ts-jest": "^27.0.5",
    "typescript": "^4.5.2"
  }
}
