{
  "name": "forager",
  "version": "0.1.1",
  "description": "",
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test:migrations": "ava test/**/*test.ts",
    "test:watch": "ava test/**/*test.ts --watch"
  },
  "keywords": [],
  "author": "Andrew Kaiser",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^7.4.3",
    "sharp": "^0.28.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.3",
    "@types/mocha": "^8.2.3",
    "@types/node": "^15.14.2",
    "@types/sharp": "^0.28.5",
    "ava": "^3.15.0",
    "date-fns": "^2.22.1",
    "mocha": "^8.4.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "ignoredByWatcher": [
      "scratchwork/*",
      "test/fixtures/*"
    ]
  }
}
