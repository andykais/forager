{
  "name": "@forager/web",
  "version": "0.0.13",
  "license": "MIT",

  "tasks": {
    "preview": "deno run -A --check --unstable-raw-imports mod.ts",
    "preview:watch": "deno run -A --check --unstable-raw-imports --watch mod.ts"
  },

  "imports": {
    "@opentelemetry/api": "npm:@opentelemetry/api@1.9.0",
    "@andykais/ts-rpc": "jsr:@andykais/ts-rpc@^0.2.2",
    "@std/http/file-server": "jsr:@std/http@0.224.5/file-server",
    "svelte": "npm:svelte@~5.39.8"

  },

  "exports": {
    ".": "./mod.ts",
    "./config": "./config.ts"
  },

  "publish": {
    "include": [
      "README.md",
      "LICENSE",
      "mod.ts",
      "config.ts",
      "server.js",
      "static/**/*",
      "server/**/*"
    ]
  }
}
