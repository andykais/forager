{
	"name": "@forager/web-impl",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "cd build && deno run --allow-env=FORAGER_CONFIG --allow-read --allow-write --allow-net=0.0.0.0:8000 mod.ts",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
    "@andykais/ts-rpc": "npm:@jsr/andykais__ts-rpc@^0.2.2",
    "@torm/sqlite": "npm:@jsr/torm__sqlite@^1.9.7",
    "@opentelemetry/api": "1.9.0"
	},
	"devDependencies": {
    "@deno/vite-plugin": "1.0.5",
		"@sveltejs/kit": "2.22.0",
		"@sveltejs/vite-plugin-svelte": "6.0.0",
		"@tailwindcss/vite": "4.0.14",
		"svelte": "5.23.2",
		"svelte-check": "4.1.5",
		"tailwindcss": "4.0.14",
		"typescript": "5.9.2",
		"vite": "7.0.4",
		"typescript-svelte-plugin": "0.3.50"
	}
}
