{
	"name": "@forager/web-impl",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "deno run -A --env-file=../../.env vite dev",
		"build": "vite build",
		"build:local": "LOCAL_BUILD=true vite build",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
    "@andykais/ts-rpc": "npm:@jsr/andykais__ts-rpc@^0.2.4",
    "@torm/sqlite": "npm:@jsr/torm__sqlite@^1.9.7",
    "@opentelemetry/api": "1.9.0"
	},
	"devDependencies": {
    "@deno/vite-plugin": "1.0.6",
		"@sveltejs/kit": "2.50.0",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tailwindcss/vite": "4.1.14",
		"svelte": "5.39.8",
		"svelte-check": "4.3.5",
		"tailwindcss": "4.1.14",
		"typescript": "5.9.3",
		"vite": "7.1.8",
		"typescript-svelte-plugin": "0.3.50"
	}
}
