{
	"name": "@forager/web-impl",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:local": "LOCAL_BUILD=true vite build",
		"preview": "cd build && deno run --allow-env=FORAGER_CONFIG --allow-read --allow-write --allow-net=0.0.0.0:8000 mod.ts",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
    "@torm/sqlite": "npm:@jsr/torm__sqlite@^1.9.7",
    "@opentelemetry/api": "1.9.0"
	},
	"devDependencies": {
    "@deno/vite-plugin": "1.0.5",
		"@sveltejs/kit": "2.43.7",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tailwindcss/vite": "4.1.14",
		"svelte": "5.39.8",
		"svelte-check": "4.3.2",
		"tailwindcss": "4.1.14",
		"typescript": "5.9.3",
		"vite": "7.1.8",
		"typescript-svelte-plugin": "0.3.50"
	}
}
