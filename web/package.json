{
	"name": "@forager/web",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"develop": "deno run -A --unstable-ffi --check npm:vite dev",
		"build": "deno run -A --unstable-ffi --check npm:vite build && cd build && deno check mod.ts",
		"preview": "deno run --config=build/deno.json -A --unstable-ffi build/mod.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"autoprefixer": "^10.4.20",
		"esbuild": "^0.23.0",
		"postcss": "^8.4.41",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.0.0",
		"typescript-svelte-plugin": "^0.3.40",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@andykais/ts-rpc": "npm:@jsr/andykais__ts-rpc@^0.2.2",
		"@forager/core": "npm:@jsr/forager__core@^0.2.2",
		"@std/yaml": "npm:@jsr/std__yaml@^1.0.4"
	}
}
